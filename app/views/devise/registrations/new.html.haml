#edit-user-form
  %h2 Settings
  = devise_error_messages!
  = form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f|
    %ul
      %li
        = f.label :email
        = f.text_field :email
        %hr

    %ul.actions
      %li= link_to "Cancel", "#", :class => "buttonStyle2"
      %li= f.submit "Save", :class => "buttonStyle2"